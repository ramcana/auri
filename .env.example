# This is an example file. Copy it to .env and fill in your actual values.

# LLM Service Configuration
LLM_SERVICE_URL=http://localhost:11434/api/chat
LLM_MODEL=mistral
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=1024
LLM_TIMEOUT=30

# Session Management
SESSIONS_FILE=sessions.json
SAVE_SESSIONS_INTERVAL=300

# Add any other API keys or configurations here
# For example:
NEWS_API_KEY=your_newsapi_org_key_here
# WEATHER_API_KEY=
